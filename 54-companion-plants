public Boolean companionPlants(String plant1, String plant2) {
  

    //code here

    Map<String, String> compPlants = new Map<String,String>();

    compPlants.put('onions', 'lettuce');

    compPlants.put('carrots','onions');

    compPlants.put('tomatoes','onions');

    compPlants.put('cucumbers', 'lettuce');

    return compPlants.get(plant1) == plant2 || compPlants.get(plant2) == plant1 ?     true:

    false;

}


        
}