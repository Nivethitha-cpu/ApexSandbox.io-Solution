public Boolean ascendingOrder(Integer a, Integer b, Integer c) {
    //code here
    if(a>b&&a>c)
    {
       if(b>c) 
       return true;
       else
       return false;
    }
    else
    {
        return false;
    }

   /* boolean isAscending;
    isAscending=(a>b&&a>c?(b>c?true:false):false);*/
}