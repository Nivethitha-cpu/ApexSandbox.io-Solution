public Boolean isLeapYear(Integer year) {
//code here
if(math.mod(year,4))
{
    return true;
}
else if(math.mod(year,100))
    {
        if(math.mod(year,400))
        {
                return true;
        }
        else{
            return false;
        }
        
    }
}
else{
    return false;
}
}
